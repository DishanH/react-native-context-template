{
  "name": "react-native-context-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "A comprehensive, production-ready React Native template with advanced authentication, theming, navigation, and robust Context-based state management",
  "scripts": {
    "start": "expo start",
    "setup-template": "node ./scripts/setup-template.js",
    "reset-project": "node ./scripts/reset-project.js",
    "clean-template": "node ./scripts/clean-template.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export -p web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "expo r -c",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.5",
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@react-native-google-signin/google-signin": "^14.0.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "53.0.11",
    "expo-auth-session": "~6.2.0",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.8",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-onboarding-swiper": "^1.3.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@types/react-native-onboarding-swiper": "^1.1.9",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "keywords": [
    "react-native",
    "expo",
    "template",
    "context",
    "context-api",
    "state-management",
    "authentication",
    "navigation",
    "typescript",
    "theme",
    "boilerplate",
    "starter",
    "onboarding",
    "secure-storage",
    "production-ready",
    "cross-platform",
    "mobile-app",
    "web-app",
    "pwa",
    "biometric-auth",
    "social-login",
    "dark-mode",
    "accessibility",
    "performance",
    "security",
    "drawer-navigation",
    "tab-navigation",
    "file-based-routing"
  ],
  "license": "MIT",
  "author": {
    "name": "React Native Context Template Team",
    "email": "support@react-native-context-template.dev",
    "url": "https://react-native-context-template.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-native-community/react-native-context-template.git"
  },
  "bugs": {
    "url": "https://github.com/react-native-community/react-native-context-template/issues"
  },
  "homepage": "https://github.com/react-native-community/react-native-context-template#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "expo": {
    "install": {
      "exclude": ["react-native-safe-area-context"]
    }
  }
}
